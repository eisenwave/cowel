\test_input{
\cowel_reinterpret_as_float(0)
\cowel_reinterpret_as_float(0x8000000000000000)
\cowel_reinterpret_as_float(0x3ff8000000000000)
\cowel_reinterpret_as_float(0xffffffffffffffff)

\test_expect_error("reinterpret.out-of-range"){\cowel_reinterpret_as_float(-1)}
\test_expect_error("reinterpret.out-of-range"){\cowel_reinterpret_as_float(0x10000000000000000)}
}

\test_output{
0
-0
1.5
NaN

<error->\cowel_reinterpret_as_float(-1)</error->
<error->\cowel_reinterpret_as_float(0x10000000000000000)</error->
}
